alerting:
  alertmanagers:
    - static_configs:
      - targets: ['localhost:9094']

rule_files:
  - rules.yml

# A list of scrape configurations.
scrape_configs:
  - job_name: "node_exporter"
    scrape_interval: 5s
    static_configs:
       - targets: 
          - localhost:9100
          - db-master:9100
          - proxy-step4j:9100
          - step4j:9100
